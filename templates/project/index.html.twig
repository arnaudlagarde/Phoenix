{% extends 'base.html.twig' %}

{% block title %}Hello !{% endblock %}

{% block body %}
    {% for project in projects %}
        <h1>Mon projet n°{{ project.id }}</h1>
        <h2>{{ project.title }}</h2>
        <h3>{{ project.description }}</h3>
        <small> crée le {{ project.createdAt | date('d/m/Y') }}</small> <br>
        <small> terminé le {{ project.endedAt | date('d/m/Y') }}</small>
    {% endfor %}

{% endblock %}
